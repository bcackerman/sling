@import "bootstrap";

body {
  padding-top: 60px;
  font: 12px arial,sans-serif;
  color: #333;
  background-color: #ebebeb;
  background-repeat: repeat;
}

form {
  margin: 0;
}


.navbar .brand {
  padding: 8px 20px 8px;
  margin-left: 0;
}


#watch-filter.nav-tabs > li {
  font-weight: bold;
}

#watch-filter.nav-tabs > li > a {
  color: #999;
}

.nav-tabs > li > a:hover {
  color: #fff;
  background-color: #111;
}

.signin-inputs input[type="checkbox"] {
  margin: -5px 0 0;
}

.signin-inputs label {
  display: inline;
}

.signin-links a {
padding-left: 0;
}

.pager {
  margin: 0;
}

.upload-input-wrap input {
  opacity: 0;
  font-size: 232px;
  height: 232px;
  position: absolute;
  right: 0px;
  top: 0px;
}


#crop-preview img, #crop-pane img {
  max-width: none;
}

.thumbnails {
margin-left: 8px;
margin-right: 0px;
}

.thumbnails > li {
margin-left: 0px;
margin-bottom: 0px;
}

#watch-guide-top .label {
  font-size: 12px;
}

.watch-featured-dropdown .label {
  font-size: 11px;
}


footer.footer {
  padding: 30px 0;
  padding-bottom: 60px;
  margin-top: 0;
}


