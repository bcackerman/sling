  background: white;
  background-image: -moz-linear-gradient(top,#fff 0,#e5e5e5 5%,#fff 55%);
  background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0,#fff),color-stop(0.05,#e5e5e5),color-stop(0.55,#fff));
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#E5E5E5',endColorstr='#FFFFFF');
  box-shadow: 0 -1px 1px #ddd;
  -moz-box-shadow: 0 -1px 1px #ddd;
  -webkit-box-shadow: 0 -1px 1px #ddd;
}



