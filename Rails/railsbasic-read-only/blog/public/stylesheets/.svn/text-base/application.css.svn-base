body {
  background-color: #FFF;
  font-family: Arial, Helvetica, sans-serif;
  font-size: 13px;
}

ol, ul {
  list-style: none;
  margin: 0;
  padding: 0;
  
}

li {
  display: list-item;
  text-align: -webkit-match-parent;
}

a {
	color: #3C6296;
	text-decoration: none;
}

a:hover {
	text-decoration: underline;
}

a img {
  border: none;
}

h1 {
  font-size: 24px;
}

h2, h2 a {
  color: #E98C08;
  font-size: 18px;
}

h3, h3 a {
